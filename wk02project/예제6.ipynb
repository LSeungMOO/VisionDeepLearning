    "import cv2 as cv\n",
    "import sys\n",
    "img=cv.imread('./laugh.jpg')\n",
    "\n",
    "if img is None:\n",
    "    sys.exit('파일을 찾을 수 없습니다.')\n",
    "\n",
    "cv.rectangle(img,(300,40),(400,190),(0,0,255),2)\n",
    "cv.putText(img,'laugh',(300,30),cv.FONT_HERSHEY_SIMPLEX,1,(255,0,0),2)\n",
    "\n",
    "cv.imshow('Draw',img)\n",
    "\n",
    "cv.waitKey()\n",
    "cv.destroyAllWindows()"
